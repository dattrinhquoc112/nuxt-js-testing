{
  "name": "beanfun-nuxt3-ssr-template",
  "private": true,
  "scripts": {
    "dev": "nuxt dev --port 3000 --host 0.0.0.0",
    "build": "nuxt build",
    "build:dev": "nuxt build --dotenv .env.dev",
    "build:test": "nuxt build --dotenv .env.test",
    "build:stg": "nuxt build --dotenv .env.stg",
    "build:prod": "nuxt build --dotenv .env.prod",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint --ext .ts,.js,.vue .",
    "lint:fix": "eslint --fix --ext .ts,.js,.vue .",
    "format": "prettier --write \"**/*.+(vue|js|ts|json)\" --ignore-path .prettierignore",
    "prepare": "husky install"
  },
  "overrides": {
    "vue": "latest"
  },
  "dependencies": {
    "@nuxtjs/google-fonts": "^3.1.3",
    "@nuxtjs/i18n": "^8.5.6",
    "@nuxtjs/robots": "^3.0.0",
    "@unleash/proxy-client-vue": "^0.1.2",
    "@vueuse/core": "^9.12.0",
    "@vueuse/nuxt": "^9.12.0",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.5",
    "html2canvas": "^1.4.1",
    "husky": "^9.1.7",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "normalize.css": "^8.0.1",
    "typescript": "^5.5.4"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@pinia/nuxt": "^0.4.6",
    "@types/crypto-js": "^4.2.2",
    "@types/js-cookie": "^3.0.2",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/lodash": "^4.17.17",
    "@types/node": "^18.11.18",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "eslint": "^8.33.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-cypress": "^2.15.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.9.0",
    "nuxt": "^3.9.0",
    "nuxt-security": "^2.0.0-beta.5",
    "pinia": "^2.0.30",
    "prettier": "^2.8.3",
    "sass-embedded": "^1.81.0",
    "scss": "^0.2.4",
    "vue-tsc": "^2.0.29"
  }
}
